<html>
  <head>
    <title>My Account</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
    <link rel='stylesheet' href='/stylesheets/bootstrap.min.css' />
    
    <script src="http://code.jquery.com/jquery-1.11.0.min.js"></script>
    <script src="http://code.jquery.com/jquery-migrate-1.2.1.min.js"></script>
    
   
  </head>
  <body>
            <header>
                <div class="header-top">
                    <div class="container">
                        <div class="welcome_name pull-left">
                            <h4>My Account</h4>
                        </div>
                        <div class="pull-right">
                            <nav class="navbar clear-right pull-right">
                                <ul class="nav nav-pills">      
                                    <li>
                                        <a href="/home" title="home" class="active">Home</a>
                                    </li>
                                  
                                    <li>
                                        <a href="/myAccount" title="My Account" class="">My Account</a>
                                    </li>
                                    
                                    <li>
                                        <a href="/myCart" title="My Cart" class="">My Cart</a>
                                    </li>

                                    <li>
                                        <a href="/logout" title="logout" class="">Logout</a>
                                    </li>
                                </ul>
                            </nav>
                        </div>
                    </div>
                </div>
            </header>   
             
           <section class="adjust container">
               
                    <table class="table table-striped">                         
                        <thead>
                            <tr>
                                <th>Product Name</th>
                                <th>Product Count</th>
                                <th>Amount</th>
                                <th></th>
                            </tr>
                        </thead>
                        
                        <tbody>
                            <% for(var i in myAccount) 
                            { %>
                                <tr>
	                                <th><%= myAccount[i].productName %></th>
	                                <th><%= myAccount[i].productCount %></th>
	                                <th><%= myAccount[i].cost %></th>
                                </tr>
                            <% } %>
                        </tbody>                        
                       
                    </table>
         
 </body>
</html>